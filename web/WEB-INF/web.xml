        <servlet-name>FreeTimeServlet</servlet-name>
        <url-pattern>/mentor-freetime</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>MenteeRatingServlet</servlet-name>
        <url-pattern>/give-feedback</url-pattern>
    </servlet-mapping>
</web-app>
